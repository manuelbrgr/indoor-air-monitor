{
  "name": "iaq-monitor",
  "description": "A web-app to visualize IAQ data",
  "version": "1.0.0",
  "private": true,
  "author": "Manuel Berger",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/material": "5.6.4",
    "axios": "0.27.2",
    "chart.js": "3.7.1",
    "chartjs-plugin-datasource": "0.1.0",
    "convert-csv-to-json": "1.3.3",
    "csv": "6.0.5",
    "d3": "7.4.4",
    "gatsby": "4.13.1",
    "gatsby-plugin-manifest": "4.13.0",
    "moment": "2.29.3",
    "react": "18.1.0",
    "react-chartjs-2": "4.1.0",
    "react-csv-to-table": "0.0.4",
    "react-d3-library": "1.1.8",
    "react-data-table-component": "7.5.0",
    "react-dom": "18.1.0",
    "react-papaparse": "4.0.2",
    "styled-components": "5.3.5",
    "tsconfig-paths-webpack-plugin": "3.5.2"
  },
  "devDependencies": {
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/react-hooks": "8.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.28",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.2",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "babel-jest": "27.5.1",
    "babel-preset-gatsby": "2.12.1",
    "commitizen": "1.0.4",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
